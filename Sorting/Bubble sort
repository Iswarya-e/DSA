 bool isSwapped=0;
        for(int i=n-1;i>0;i--)
        {
            for(int j=1;j<=i;j++)
            {
                if(nums[j-1]>nums[j]) 
                {
                    swap(nums[j],nums[j-1]);
                    isSwapped=1;
                }
            }
            if(isSwapped==0) break;
        }

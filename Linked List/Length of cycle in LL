int countNodesinLoop(struct Node *head)
{
    Node* fast=head;
    Node* slow=head;
    bool isCycle=false;
    
    while(fast!=NULL and fast->next!=NULL)
    {
        fast=fast->next->next;
        slow=slow->next;
        if(slow==fast)
        {
            isCycle=true;
            break;
        }
    }
    if(!isCycle) return 0;
    
    int count=1;
    slow=slow->next;
    while(slow!=fast)
    {
       count++;
       slow=slow->next;
    }
    return count;
    // Code here
}

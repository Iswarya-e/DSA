
class Solution {
public:
    ListNode* deleteDuplicates(ListNode* head) {
        ListNode* ans=head;
        ListNode* traverser=head;
        if(head==NULL) return NULL;
        while(traverser!=NULL)
        {
            if(traverser->val!=ans->val){
                ans->next=traverser;
                ans=ans->next;
            }
            traverser=traverser->next;
        }
        ans->next=NULL;
        return head;
    }
};
